# ---------- RPC & Wallet ----------
# HTTPS endpoint for Base Sepolia transactions (Alchemy, Infura, etc.)
BASE_SEPOLIA_RPC_URL=

# Hex-encoded private key for the solver account (no 0x prefix required by viem)
SOLVER_PRIVATE_KEY=

# Address of the deployed IntentHub contract on Base Sepolia
INTENT_HUB_ADDRESS=

# Collateral (wei) staked with each commitment (uint96)
SOLVER_COLLATERAL_WEI=

# ---------- BlockLock / Conditional Encryption ----------
# API key or auth token for blocklock-js (if required by your deployment)
BLOCKLOCK_API_KEY=

# BlockLock subscription ID (leave empty if using direct funding)
BLOCKLOCK_SUBSCRIPTION_ID=

# BlockLock sender address per chain (Base Sepolia shown here)
BLOCKLOCK_SENDER_BASE_SEPOLIA=0x82Fed730CbdeC5A2D8724F2e3b316a70A565e27e

# Optional override for blocklock-js encrypt endpoint
BLOCKLOCK_ENCRYPTION_ENDPOINT=

# Optional override for callback gas limit (defaults to 300000)
BLOCKLOCK_CALLBACK_GAS_LIMIT=

# Optional additional buffer percent applied to BlockLock request price (defaults to SDK value)
BLOCKLOCK_BUFFER_PERCENT=

# ---------- Intent Ingestion Sources ----------
# REST endpoints for intent feeds (external sources optional; manual/mock recommended)
COW_API_URL=# optional, leave blank to disable
ONEINCH_API_URL=# optional, leave blank to disable
MANUAL_INTENT_URL=
LISTENER_API_URL=http://localhost:4001

# ---------- Venue Quote APIs ----------
# Optional HTTP endpoints that back the Aerodrome / Uniswap / Curve connectors
AERODROME_API_URL=
UNISWAP_API_URL=
CURVE_API_URL=

# ---------- Execution / Relay ----------
# Private relay or preferred RPC for settlement transactions (leave blank to reuse BASE_SEPOLIA_RPC_URL)
PRIVATE_RELAY_URL=
PRIVATE_RELAY_AUTH_TOKEN=

# Optional mock bridge configuration (JSON string or path interpreted by app-specific logic)
MOCK_BRIDGE_CONFIG=[{"destinationVenue":"mock-bridge","chainId":84532,"expectedAmountOut":"1050000000000000000","gasEstimate":"120000","bridgeFee":"300000000000000","context":{"note":"local mock config"}}]

# ---------- Dashboard / Frontend ----------
# RPC exposed to the dashboard wagmi provider
NEXT_PUBLIC_BASE_SEPOLIA_RPC_URL=
MOCK_INTENT_RANDOM_COUNT=4
PLANNER_RANDOM_COUNT=3

# ---------- Telemetry / Instrumentation ----------
SOLVER_TELEMETRY_FILE=../.cache/solver-telemetry.json


